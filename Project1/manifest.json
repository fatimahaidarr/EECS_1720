{
    "name": "project2",
    "version":"0.1",
    "manifest_version": 2,
    "content_scripts":[
    
     {
         "matches":[
             "<all_urls>"
        ],

         "js": ["content.js"]
     }
    ],
    
    "browser_action":{
        "default_icon": "spider.png",
        "default_popup":"popup.html",
        "defualt_title":"A popup will come here."
    },
    "permissions": [
        "storage",
        "default_popup"
      ]
    
}

    